<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id='sh_pharmx_gateways_tree' model='ir.ui.view'>
        <field name="name">sh.pharmx.gateways.tree</field>
        <field name="model">pharmx.gateways</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="url" />
                <field name="available" />
                <field name="priority" />
            </tree>
        </field>
    </record>
    <record id="sh_pharmx_gateways_action" model="ir.actions.act_window">
        <field name="name">PharmX gateways</field>
        <field name="res_model">pharmx.gateways</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Gateways
            </p>
        </field>
    </record>
</odoo>