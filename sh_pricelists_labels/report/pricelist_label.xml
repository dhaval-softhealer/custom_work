<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="report_product_pricelist_label" model="ir.actions.report">
            <field name="name">Product Label (PDF)</field>
            <field name="model">product.pricelist</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">sh_pricelists_labels.report_productpricelistlabel</field>
            <field name="report_file">sh_pricelists_labels.report_productpricelistlabel</field>
            <field name="paperformat_id" ref="product.paperformat_label_sheet"/>
            <field name="print_report_name">'Products Labels - %s' % (object.name)</field>
            <field name="binding_model_id" eval="False"/>
            <field name="binding_type">report</field>
        </record>

        <record id="report_product_pricelist_label_dymo" model="ir.actions.report">
            <field name="name">Product Label (PDF)</field>
            <field name="model">product.pricelist</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">sh_pricelists_labels.report_productpricelistlabel_dymo</field>
            <field name="report_file">sh_pricelists_labels.report_productpricelistlabel_dymo</field>
            <field name="paperformat_id" ref="product.paperformat_label_sheet_dymo"/>
            <field name="print_report_name">'Products Labels - %s' % (object.name)</field>
            <field name="binding_type">report</field>
        </record>


        <record id="label_pricelist_product" model="ir.actions.report">
            <field name="name">Product Label (ZPL)</field>
            <field name="model">product.pricelist</field>
            <field name="report_type">qweb-text</field>
            <field name="report_name">sh_pricelists_labels.label_product_pricelist_view</field>
            <field name="report_file">sh_pricelists_labels.label_product_pricelist_view</field>
            <field name="binding_model_id" eval="False"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
