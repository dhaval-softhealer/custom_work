<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Channels -->
    <record id="datasync_channel" model="queue.job.channel">
        <field name="name">DataSync</field>
        <field name="parent_id" ref="queue_job.channel_root" />
    </record>

    <!-- Jobs -->
    <record id="datasync_incoming_message" model="queue.job.function">
        <field name="model_id" ref="pharmx_edi.model_datasync_message"></field>
        <field name="method">process</field>
        <field name="channel_id" ref="datasync_channel" />
        <field name="retry_pattern" eval="{1: 60, 2: 180, 3: 10, 5: 300}" />
    </record>

</odoo>
